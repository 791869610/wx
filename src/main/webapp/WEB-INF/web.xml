<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee   http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>WxAuth</display-name>
  <servlet>
    <servlet-name>wxCallBack</servlet-name>
    <servlet-class>com.atwjsw.wx.auth.servlet.CallBackServlet</servlet-class>
    <init-param>
      <param-name>dbUrl</param-name>
      <param-value>jdbc:mysql://127.0.0.1:3306/auth</param-value>
    </init-param>
    <init-param>
      <param-name>driverName</param-name>
      <param-value>com.mysql.jdbc.Driver</param-value>
    </init-param>
    <init-param>
      <param-name>userName</param-name>
      <param-value>root</param-value>
    </init-param>
    <init-param>
      <param-name>passWord</param-name>
      <param-value>123456</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>wxCallBack</servlet-name>
    <url-pattern>/wxCallBack</url-pattern>
  </servlet-mapping>
</web-app>
